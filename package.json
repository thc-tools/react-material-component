        "@storybook/addons": "4.0.0-alpha.15",
        "@storybook/react": "4.0.0-alpha.15",
        "@types/enzyme": "3.1.13",
        "@types/jest": "23.3.1",
        "@types/lodash": "4.14.116",
        "@types/material-components-web": "0.35.0",
        "@types/react": "16.4.7",
        "@types/storybook__addon-actions": "3.4.0",
        "@types/storybook__addon-info": "3.4.0",
        "@types/storybook__react": "3.0.8",
        "babel-core": "6.26.3",
        "babel-jest": "23.4.2",
        "babel-preset-env": "1.7.0",
        "babel-preset-react": "6.24.1",
        "conventional-github-releaser": "3.1.2",
        "enzyme": "3.3.0",
        "enzyme-adapter-react-16": "1.1.1",
